<h1>Lista de Pacientes</h1>
<table>
    <tr>
        <th>Nombre</th>
        <th>Apellido</th>
        <th>DNI</th>
        <th>Obra Social</th>
        <th>Tel√©fono</th>
        <th>Email</th>
        <th>Acciones</th>
    </tr>
    <% pacientes.forEach(paciente => { %>
        <tr>
            <td><%= paciente.Nombre_Paciente %></td>
            <td><%= paciente.Apellido_Paciente %></td>
            <td><%= paciente.Dni_Paciente %></td>
            <td><%= paciente.Obra_Social %></td>
            <td><%= paciente.Telefono %></td>
            <td><%= paciente.Email %></td>
            <td>
                <a href="/paciente/editar/<%= paciente.ID_Paciente %>">Editar</a> |
                <a href="/paciente/baja/<%= paciente.ID_Paciente %>">Eliminar</a>
            </td>
        </tr>
    <% }) %>
</table>
<a href="/paciente/agregar">Agregar Paciente</a>
